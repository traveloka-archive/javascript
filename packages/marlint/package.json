{
  "name": "marlint",
  "version": "6.7.1",
  "description": "Javascript code linter for traveloka js styleguide",
  "license": "MIT",
  "repository": "traveloka/javascript",
  "author": {
    "name": "Traveloka",
    "url": "traveloka.com"
  },
  "bin": "cli.js",
  "scripts": {
    "test": "jest"
  },
  "files": [
    "*.js",
    "lib"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "marlint",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "simple"
  ],
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@typescript-eslint/parser": "^1.5.0",
    "arrify": "^1.0.1",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.16.0",
    "eslint-config-marlint": "^6.5.1",
    "eslint-config-prettier": "^2.10.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-json": "^2.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-marlint": "^0.1.3",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "globby": "^6.1.0",
    "jest-worker": "^21.2.1",
    "lodash.merge": "^4.6.1",
    "meow": "^3.7.0",
    "minimatch": "^3.0.4",
    "pkg-conf": "^2.1.0",
    "typescript": "^3.3.4000",
    "update-notifier": "^2.5.0"
  },
  "marlint": {
    "rules": {
      "no-console": 0,
      "no-empty": 0,
      "no-sync": 0
    }
  },
  "devDependencies": {
    "jest": "^22.4.4"
  },
  "jest": {
    "testURL": "http://localhost"
  }
